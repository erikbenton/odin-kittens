<%= form_for(@kitten, url: url) do |f| %>
	<%= f.label :name, "Name" %>
	<%= f.text_field :name %>

	<%= f.label :age, "Age" %>
	<%= f.number_field :age %>

	<%= f.label :cuteness, "Cuteness (out of 10)" %>
	<%= f.number_field :cuteness %>

	<%= f.label :softness, "Softness (out of 10)" %>
	<%= f.number_field :softness %>

	<%= f.submit "Submit" %>
<% end %>